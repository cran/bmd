citHeader("To cite the package 'bmd' in publications use:")

citEntry(entry="Unpublished",
         title = "Benchmark dose analysis in R",
         author = personList(as.person("C. Ritz")),
         year = 2008,
         textVersion =
         paste("Ritz, C. (2008)",
               "Benchmark dose analysis in R",
               "Manuscript")
)
